#!/bin/bash

ZERO_ARG_BASENAME=$(basename $0)

function warn_local
{
    if [ "$DTS_DEBUG" ]; then

        echo "[WARN, $ZERO_ARG_BASENAME] : {" $@ "}"

    fi
}

warn_local "ARGS: $@"

DTS_APP=$1

if [ ! "$DTS_APP" ]; then
    DTS_APP=app1
fi
    
QML_IMPORT_PATH=$QML_IMPORT_PATH:$DTS_DIR_CACHE_QML

QML_FILE=$DTS_DIR/ui/unitTests/V0001/ComHyraAppDevTimeStudio/${DTS_APP}-${PARAM_ORGANISM}.qml

warn_local PARAM_ORGANISM=$PARAM_ORGANISM

warn_local DTS_DIR=$DTS_DIR

warn_local QML_FILE=$QML_FILE

warn_local Set QML_IMPORT_PATH=$QML_IMPORT_PATH

warn_local qml --desktop $QML_FILE 2\>$DTS_DIR/dts_command_ui_open.stderr

qml --desktop $QML_FILE 2>$DTS_DIR/dts_command_ui_open.stderr

warn_local "qml exit value=$?"

warn_local "Saved stderr: $DTS_DIR/dts_command_ui_open.stderr"


