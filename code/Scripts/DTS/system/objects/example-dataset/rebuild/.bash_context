    find $DTS_DIR_PROJECTS/$PARAM_ORGANISM/unified/MATs/TN_*.mat | parallel dts call system/objects/example-dataset/build_plot_write_to_disk system/contexts/example-dataset/$PARAM_ORGANISM/parametersweep1/pset1       {}
